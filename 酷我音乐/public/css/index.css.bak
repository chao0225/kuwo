/*banner.js  轮播*/
.banner{
	width: 1200px; 
	/* å¤‡ç”¨èƒŒæ™¯è‰² */
	/*background-image: linear-gradient(to top, #434343 0%, black 100%);*/
	/* çŽ°ç”¨èƒŒæ™¯è‰² */
	/* background-image: linear-gradient(60deg, #29323c 0%, #485563 100%); */
	color: white;
	margin:0 auto;
	overflow:hidden;
	position: relative;
	-webkit-perspective: 500px; 
}

.banner_bg {
    background-image: linear-gradient(60deg, #29323c 0%, #485563 100%);
}

.banner .wrap{
	width: 730px;
	height: 370px;
	margin: 0 auto;
	display:flex;
	flex-direction: column;
	align-items: center;
	-moz-perspective: 500px; 
	
}
.banner .download{
	width: 252px;
	height: 336px;
}

.banner .wrap > *{
	flex: 0 0 auto;
}

.banner .ban{
	/*width:40%;*/
	z-index: 10;
    margin-top: 8px;
	 transform-style:preserve-3d; 
	transition:transform 0.5s;
}
.banner .ban img{
	box-shadow: 0px 8px 70px rgba(0, 0, 0, 1);
	width: 100%;
    box-sizing: border-box;
    padding: 0 0px;
}
.banner .ban>img:not(:first-of-type){
	position: absolute;
    left: 0;
    top: 0;
}
.banner .shadow{
	position: absolute;
    bottom: 0;
    z-index:-1;
    left: 0;
    width:1200px;
}
.banner .shadow>img{
	width:100%
}
.banner .dots{
	display:flex;
	height: 15px;
    z-index: 11;
    /* position: absolute;
    bottom: 2.5%;
    right: 35%; */
}
.banner .dots li{
	cursor: pointer;
	width: 15px;
	height: 15px;
	border-radius: 50%;
	box-shadow: 0 0 10px rgba(255, 255, 255, 0.7);
	margin:10px 0 0 40px
}
.banner .dots li:hover{
	box-shadow: 0 0 10px rgba(255, 255, 255, 0.8);
	background:black;
	transition: all 0.5s ease;
}
.banner .dots li.active{
	box-shadow: 0 0 20px rgba(255, 80, 90, 0.6);
	background: rgb(23, 0, 0);
	transition: all 0.5s ease;
}
.banner .btn{
	display: flex;
}
.banner .btn>*{
	transform: scale(1.3);
	background-color: transparent;
	cursor: pointer;
	background-image: url(../img/lunbo/banner.png);
	position:absolute;
	top: 37%;
	width: 37px;
	height: 63px;
}
/* é¼ æ ‡åˆ’å…¥btnåŠ¨ç”» */
.banner .btn>.prev,
.banner .btn>.next{
	transition: left 0.3s ease-in,
				right 0.3s ease-in;
}
.banner:hover .btn>.prev {
	left: 0px;
}
.banner:hover .btn>.next {
	right: 0px;
	
}
.banner .btn>.prev{
	left: -36px;
	background-position: 0 -360px;
    outline: none;
}
.banner .btn>.prev:hover{
	background-position: 0 -430px;
}
.banner .btn>.next{
	right: -36px;
	background-position: 0 -508px;
    outline: none;
}
.banner .btn>.next:hover{
	background-position: 0 -578px;
}

/*  两张宣传图  10zj.js */
.banner_10:after{
	content:"";
	display:block;
	clear:both;
}
.banner_10{
	position: relative;
}
.banner_10 .banner1 ,.banner_10 .banner2  {
		width: 580px;
		height: 320px;
		box-sizing: border-box;
		overflow: hidden;
		position: absolute;
		left:-60px;
		top:-320px;
	}
	.banner_10 .banner2 {
		left:600px;
	}
	.banner1 ul, .banner2 ul{
		list-style: none;
		overflow: hidden;
		margin: 0;
		padding: 0;
		position: relative;
		z-index: 5;
	}
	.banner1 ul li, .banner2 ul li{
		position: relative;
		float: left;
		width: 290px;
		height: 160px;
		overflow: hidden;
	}
	
	.banner1 .banner1_bg, .banner2 .banner2_bg{
		position: absolute;
		top: 0;
		left: 0;
		width: 580px;
		height: 320px;
		z-index: 1;
		transform: scale(1.5);
		transition-duration: 1s;
	}
	.banner1 .banner1_bg{
		background: url("../img/lunboxia2/3.jpg") no-repeat;
	}
	.banner2 .banner2_bg{
		background: url("../img/lunboxia2/3.jpg") no-repeat;
	}
	/*表面*/
	.banner1 ul li span, .banner2 ul li span {
		display: inline-block;
		width: 290px;
		height: 160px;
		transition-duration: 1s;
	}
	.banner1 ul li:nth-child(1) span{
		background: url("../img/lunboxia2/1.jpg") no-repeat top left;
	}
	.banner1 ul li:nth-child(2) span{
		background: url("../img/lunboxia2/1.jpg") no-repeat top right;
		
	}
	.banner1 ul li:nth-child(3) span{
		background: url("../img/lunboxia2/1.jpg") no-repeat bottom left;
	}
	.banner1 ul li:nth-child(4) span{
		background: url("../img/lunboxia2/1.jpg") no-repeat bottom right;
	}
	/*第二张*/
	.banner2 ul li:nth-child(1) span{
		background: url("../img/lunboxia2/2.jpg") no-repeat top left;
	}
	.banner2 ul li:nth-child(2) span{
		background: url("../img/lunboxia2/2.jpg") no-repeat top right;
	}
	.banner2 ul li:nth-child(3) span{
		background: url("../img/lunboxia2/2.jpg") no-repeat bottom left;
	}
	.banner2 ul li:nth-child(4) span{
		background: url("../img/lunboxia2/2.jpg") no-repeat bottom right;
	}
	/*hover*/
	.banner1:hover .banner1_bg,.banner2:hover .banner2_bg{
		transform:scale(1);
		transition-duration:1s;
	}
	.banner1:hover ul li:nth-child(1) span,.banner2:hover ul li:nth-child(1) span{
		transform:translate(0,160px);
		transition-duration:1s;
	}
	.banner1:hover ul li:nth-child(2) span,.banner2:hover ul li:nth-child(2) span{
		transform:translate(-290px,0); 
		transition-duration:1s; 
	}
	.banner1:hover ul li:nth-child(3) span,.banner2:hover ul li:nth-child(3) span{
		transform:translate(290px,0); 
		transition-duration:1s; 
	}
	.banner1:hover ul li:nth-child(4) span,.banner2:hover ul li:nth-child(4) span{
		transform:translate(0px,-160px); 
		transition-duration:1s; 
    }
    

