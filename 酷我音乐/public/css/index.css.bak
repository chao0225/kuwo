}

#gedan li div:after{
    opacity: 0;
    visibility: hidden;
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.3);
    z-index: 8;
    transition: 0.8s;
}
#gedan li:hover div:after{
    opacity: 1;
    visibility: visible;
}
